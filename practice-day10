adding = 4 + 3
print(adding)

subtraction = 8 - 9
print(subtraction)

multiplication = 3.4 * 6.8
print(multiplication)

division = 2467 / 4673
print(division)

# a number raised to the power of some number
# in this example we raise 10 to the power of 2
squared = 10**2
print(squared)

# remainder of a division
modulo = 343 % 4
print(modulo)

# whole number division
divisor = 15 // 2
print(divisor)